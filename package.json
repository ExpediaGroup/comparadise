{
  "name": "comparadise",
  "proxy": "http://localhost:8080",
  "dependencies": {
    "@aws-sdk/client-s3": "3.178.0",
    "@emotion/react": "11.10.5",
    "@emotion/styled": "11.10.5",
    "@mui/icons-material": "5.10.9",
    "@mui/material": "5.10.11",
    "@octokit/rest": "19.0.5",
    "@trpc/client": "10.3.0",
    "@trpc/react-query": "10.3.0",
    "@trpc/server": "10.3.0",
    "@types/cors": "2.8.12",
    "@types/express": "4.17.15",
    "@types/lodash.groupby": "4.6.7",
    "@types/node": "18.7.21",
    "@types/react": "18.0.21",
    "@types/react-dom": "18.0.8",
    "cors": "2.8.5",
    "express-rate-limit": "6.6.0",
    "lodash.groupby": "4.6.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-scripts": "5.0.1",
    "ts-node": "10.9.1",
    "typescript": "4.9.3",
    "use-query-params": "2.1.2",
    "zod": "3.19.1"
  },
  "devDependencies": {
    "@testing-library/cypress": "9.0.0",
    "@types/jest": "28.1.7",
    "@types/testing-library__cypress": "5.0.9",
    "cypress": "12.7.0",
    "jest": "28.1.3",
    "npm-watch": "0.11.0",
    "prettier": "2.8.0",
    "start-server-and-test": "1.15.5",
    "ts-jest": "28.0.8"
  },
  "scripts": {
    "build": "react-scripts build",
    "build:docker": "docker build . --tag comparadise && docker-compose up",
    "cypress:e2e": "cypress run --e2e",
    "cypress:open": "CYPRESS_BASE_URL=http://localhost:3000 cypress open",
    "dev": "DISABLE_ESLINT_PLUGIN=true npm run start:client & npm-watch",
    "format": "prettier --write .",
    "format-check": "prettier --check .",
    "lint": "eslint .",
    "start:client": "react-scripts start",
    "start:server": "ts-node server/index.ts",
    "test:client": "cypress run --component",
    "test:integration": "start-server-and-test build:docker http://localhost:8080 cypress:e2e",
    "test:server": "jest --silent"
  },
  "watch": {
    "start:server": {
      "patterns": [
        "server"
      ],
      "extensions": "ts"
    }
  },
  "jest": {
    "transform": {
      "^.+\\.(j|t)sx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "clearMocks": true
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
